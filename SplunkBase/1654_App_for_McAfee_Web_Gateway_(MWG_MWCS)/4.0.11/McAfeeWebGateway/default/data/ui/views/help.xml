   <option value="Other">Other</option>
 </select>
 <br/>
 <label for="comments">Your message:</label>
 <textarea name="comments" id="comments" cols="70" rows="3" class="form-control" style="min-width: 100%"></textarea>
 <br/>

 <label for="rate_documentation">Please rate the documentation:</label>
 <textarea name="rate_documentation" id="rate_documentation" cols="70" rows="1" class="form-control" style="min-width: 100%"></textarea>
 <br/>
 <label for="rate_installation">Please rate installation/configuration:</label>
 <textarea name="rate_installation" id="comments" cols="70" rows="1" class="form-control" style="min-width: 100%"></textarea>
 <br/>
 <label for="rate_usability">Please rate usability:</label>
 <textarea name="rate_usability" id="rate_usability" cols="70" rows="1" class="form-control" style="min-width: 100%"></textarea>
 <br/>

 <input type="submit" name="submit" value="Send" />
 <input type="reset" name="reset" value="Clear form" />
</form>

