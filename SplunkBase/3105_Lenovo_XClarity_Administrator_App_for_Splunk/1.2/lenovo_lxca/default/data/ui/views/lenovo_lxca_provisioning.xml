        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <title>Number of OS images deployed from XClarity over time</title>
      <chart>
        <title>Number of OS images deployed from XClarity Administrator instances over time</title>
        <search>
          <query>index=lenovo_lxca appl=LXCA  EventID=FQXHMFC8877I | timechart count</query>
