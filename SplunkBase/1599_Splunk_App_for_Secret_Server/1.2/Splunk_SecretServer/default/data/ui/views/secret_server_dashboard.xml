<?xml version="1.0" encoding="utf-8"?><dashboard>
  <label>Secret Server Dashboard</label>
  <!-- First row of a single comment -->
  <!-- Second row of 2 panels -->
  <row>
    <panel>
      <chart>
        <title>Top Users in the Last 30 Days</title>
        <search ref="Top Users"></search>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.showMarkers">false</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Heartbeat Failures</title>
        <search ref="Heartbeat Failures in the last 24 Hours"></search>
        <option name="charting.axisTitleY.text">## of failures</option>
        <option name="charting.chart">column</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.showMarkers">false</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <event>
        <title>Secret Server Events</title>
        <search ref="Thycotic Events">
          <earliest></earliest>
          <latest></latest>
        </search>
        <option name="count">10</option>
        <option name="maxLines">10</option>
      </event>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Top 10 Events in the Last 24 Hours</title>
        <search>
          <query>source="secretserver" | where message_name != "System Log" | top limit=10 message_name</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Secret Views in last 48 Hours</title>
        <search ref="User Views"></search>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Lockouts Over the Last Week</title>
        <search>
          <query>source="secretserver" | where signature_id = 4 | timechart count span=1d</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Login Failures Over the Last Week</title>
        <search>
          <query>source="secretserver" | where signature_id = 18 | timechart count span=1d</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Top 10 Secrets in the Last Day</title>
        <search>
          <query>source="secretserver" signature_id=10004 | top limit=10 container_name, fname</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>
</dashboard>